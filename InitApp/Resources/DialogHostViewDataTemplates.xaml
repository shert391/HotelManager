<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dialogHostViewModels="clr-namespace:HotelManager.MVVM.ViewModels.DialogHostViewModels"
    xmlns:dialogHostViews="clr-namespace:HotelManager.MVVM.Views.Xamls.DialogHostViews">
    <DataTemplate DataType="{x:Type dialogHostViewModels:RoomCreatorViewModel}">
        <dialogHostViews:RoomCreator />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogHostViewModels:MessageBoxInformationViewModel}">
        <dialogHostViews:MessageBoxInformation />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogHostViewModels:MessageBoxConfirmationViewModel}">
        <dialogHostViews:MessageBoxConfirmation />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogHostViewModels:RoomEditorViewModel}">
        <dialogHostViews:RoomEditor />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogHostViewModels:ReserveRoomViewModel}">
        <dialogHostViews:ReserveRoom />
    </DataTemplate>

    <DataTemplate DataType="{x:Type dialogHostViewModels:PeopleCreatorViewModel}">
        <dialogHostViews:PeopleCreator />
    </DataTemplate>
</ResourceDictionary>